{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "apigateway:*",
                "application-autoscaling:*",
                "autoscaling:*",
                "cloudfront:Get*",
                "cloudfront:List*",
                "cloudformation:Describe*",
                "cloudformation:Get*",
                "cloudformation:List*",
                "cloudwatch:*",
                "dynamodb:BatchGetItem",
                "dynamodb:Describe*",
                "dynamodb:Get*",
                "dynamodb:List*",
                "dynamodb:Scan",
                "dynamodb:Query",
                "ec2:Describe*",
                "ec2:Get*",
                "elasticbeanstalk:*",
                "elasticache:Describe*",
                "elasticache:List*",
                "elasticloadbalancing:*",
                "iam:List*",
                "iam:Get*",
                "lambda:Get*",
                "lambda:List*",
                "logs:*",
                "rds:Describe*",
                "rds:Download*",
                "rds:List*",
                "s3:*"
            ],
            "Resource": "*"
        }
    ]
}